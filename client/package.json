    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "compromise": "^14.12.0",
    "framer-motion": "^11.0.6",
    "lucide-react": "^0.288.0",
    "powerthesaurus-api": "^2.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
