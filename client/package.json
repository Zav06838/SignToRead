  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jest-environment-jsdom",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.test.{js,jsx,ts,tsx}"
    ],
    "transformIgnorePatterns": [
      "/node_modules/",
      "\\.(css|sass|scss|less)$"
    ]
